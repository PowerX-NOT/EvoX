on boot
    chown system system /sys/devices/platform/soc/soc:qcom,dsi-display/hbm
    chown system system /sys/devices/platform/soc/soc:qcom,dsi-display/dc_dim
    chmod 0660 /sys/devices/platform/soc/soc:qcom,dsi-display/hbm
    chmod 0660 /sys/devices/platform/soc/soc:qcom,dsi-display/dc_dim

service vendor.livedisplay-hal-2-1 /vendor/bin/hw/vendor.lineage.livedisplay@2.1-service.laurel_sprout
    class hal
    user system
    group system
